//- Portfolio
section#portfolio
    .container
        .row
            .col-lg-12.text-center
                h2.section-heading.text-uppercase.pb-3.quickSand-title.text-primary Portfolio
                //h3.section-subheading.text-muted Lorem ipsum dolor sit amet consectetur.
        .row
            .col-md-12
                ul#myTab.nav.nav-tabs(role='tablist')
                    li.nav-item
                        a#examples-tab.nav-link.active(data-toggle='tab', href='#ejemplos', role='tab', aria-controls='examples', aria-selected='true') Ejemplos
                    li.nav-item
                        a#samples-tab.nav-link(data-toggle='tab', href='#muestra', role='tab', aria-controls='samples', aria-selected='false') Muestra app almacen
                    li.nav-item
                        a#files-management-tab.nav-link(data-toggle='tab', href='#files-management', role='tab', aria-controls='files-management', aria-selected='false') Gestion de archivos
                    li.nav-item
                        a#ecommerce-tab.nav-link(data-toggle='tab', href='#ecommerce', role='tab', aria-controls='files-management', aria-selected='false') E-commerce

                .card.bg-transparent.border-0
                    .card-body
                        .tab-content#tablesGroup
                            include tabs/examples
                            include tabs/samples
                            include tabs/files-management
                            include tabs/e-commerce
    #test
        svg#svg
